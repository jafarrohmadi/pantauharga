    <link href="<g:resource dir="css" absolute="true"/>/bootstrap.css" type="text/css" rel="stylesheet">
    <link href="<g:resource dir="css" absolute="true"/>/bootstrap-responsive.css" type="text/css"
          rel="stylesheet">
    <link href="<g:resource dir="css" absolute="true"/>/bootstrap-box.css" type="text/css" rel="stylesheet"/>
    <link href="<g:resource dir="css" absolute="true"/>/font-awesome.css" type="text/css" rel="stylesheet"/>
    <link href="<g:resource dir="css" absolute="true"/>/base.css" type="text/css" rel="stylesheet">
    <link href="<g:resource dir="css" absolute="true"/>/style.css" type="text/css" rel="stylesheet">

</head>

<body>
<div id="main_wrapper">
    <div class="main_wrapper_inner">

        <div class="header_top_second navbar-fixed-top">
            <div class="container header_top_second_inner">
                <div class="row">
                    <div class="span12">
                        <!-- Header -->
                        <div class="header_top_inner1">
                            <div class="toad_wrapper">
                                <div class="align_left logo"><a href="#header"><img
                                        src="<g:resource dir="images" absolute="true"/>/logo.png" alt=""/></a>
                                </div>
                            </div>

                            <div class="main_menu">
                                <!-- mobile menu -->
                                <button type="button" class="btn btn-navbar" data-toggle="collapse"
                                        data-target=".nav-collapse">
                                    <i class="icons-reorder"></i>
                                </button>
                                <!-- desktop menu -->
                                <div class="nav-collapse collapse">
                                    <nav id="dropdown">
                                        <ul class="sf-menu clearfix nav">
                                            <li class="current"><a href="#header" class="trigger"><span>Home</span></a>
                                            </li>
                                            <!-- <li><a href="#services" class="trigger"><span>Services</span></a></li> -->
                                            <li><a href="#about" class="trigger"><span>About</span></a></li>
                                            <li><a href="#map" class="trigger"><span>Map</span></a></li>
                                            <!-- <li><a href="#inflasi" class="trigger"><span>Check Inflasi</span></a></li> -->
                                            <li><a href="#download" class="trigger"><span>Download</span></a></li>
                                            <li><a href="#contact" class="trigger"><span>Contact</span></a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                            <!-- end navigation -->
                        </div>

                    </div>
                </div>
            </div>

            <div class="clear"></div>
        </div>

        <!-- Start slider  -->
        <section id="header">
            <div class="content_wrapper_gray landing-hero">
                <div class="container">
                    <div class="main_title_wrapper">
                        <div class="block_tile_wrapper">
                            <div class="throw">
                                <img src="<g:resource dir="images" absolute="true"/>/banana.png">

                                <h1 class="block_tile"><span class="text-yellow">APLIKASI</span> PANTAU HARGA</h1>

                                <div class=""><hr></div>
                                <h6 class="block_tile_2">
                                    Mendekatkan petani dengan konsumen akhir<br>
                                </h6>

                                <div class="slide_down">
                                    <a href="#about">EXPLORE</a>
                                </div>
                            </div>
                        </div>

                        <div class="clear"></div>
                    </div>
                </div>
            </div>
        </section>

        <div class="content">
            <div class="content_wrapper">
                <g:layoutBody/>
            </div><!-- End Content wrapper -->
        </div><!-- End content -->


        <footer id="footer">
            <div class="container">
                <div class="footer_inner">
                    <div class="row">
                        <div class="span12">
                            <div class="footer_right">
                                <div class="clear"></div>


                                <div class="pull-left"><img src="<g:resource dir="images" file="logo@2x.png"/>"/></div>
                                Copyright &copy; ${new Date().format("yyyy")} Team PantauHarga </br>
                                All Right Reserved
                            </div>
                            <div class="clear"></div>
                        </div>

                    </div>
                </div>
            </div>
        </footer> <!-- End Footer -->
    </div> <!-- End main_wrapper_inner -->
</div> <!-- End main_wrapper -->

<div class="clear"></div>


